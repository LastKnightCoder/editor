{
  "build": {
    "beforeBuildCommand": "npm run build",
    "beforeDevCommand": "npm run dev",
    "devPath": "http://localhost:5173",
    "distDir": "../dist"
  },
  "package": {
    "productName": "editor",
    "version": "0.4.13"
  },
  "tauri": {
    "allowlist": {
      "shell": {
        "all": true,
        "execute": true,
        "sidecar": true,
        "open": true
      },
      "window": {
        "all": false,
        "close": true,
        "hide": true,
        "show": true,
        "maximize": true,
        "minimize": true,
        "unmaximize": true,
        "unminimize": true,
        "startDragging": true,
        "setFullscreen": true
      },
      "dialog": {
        "all": true,
        "ask": true,
        "confirm": true,
        "message": true,
        "open": true,
        "save": true
      },
      "os": {
        "all": true
      },
      "fs": {
        "all": true,
        "scope": [
          "$HOME",
          "$HOME/**/*",
          "$HOME/.editor",
          "$HOME/.editor/**/*",
          "$HOME/.editor/backup",
          "$HOME/.editor/backup/**/*"
        ]
      },
      "process": {
        "all": true
      },
      "path": {
        "all": true
      }
    },
    "bundle": {
      "active": true,
      "category": "DeveloperTool",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "tao",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null,
        "minimumSystemVersion": "13.3"
      },
      "resources": [],
      "shortDescription": "",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": true,
      "endpoints": [
        "https://github.com/LastKnightCoder/editor/releases/latest/download/latest.json"
      ],
      "windows": {
        "installMode": "passive"
      },
      "dialog": false,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDU3NTg4MjY2RUI5MEUzOTIKUldTUzQ1RHJab0pZVi91OVhvSEFra3pwc1hCM2o1L2ZhZktqUTFTMlZBWlo0K0dLa2VMM3FYUloK"
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 640,
        "resizable": true,
        "title": "editor",
        "hiddenTitle": true,
        "width": 920,
        "center": true,
        "decorations": false,
        "fileDropEnabled": false,
        "transparent": true
      }
    ]
  }
}